{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/3.5/**.dll"
          ]
        }
      ],
      "dest": "3.5"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "3.5/**.yml"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "README.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "docs",
    "globalMetadata": {
      "_appTitle": "Godot C# API",
      "_appLogoPath": "images/documentation_logo.svg",
      "_appFaviconPath": "images/documentation_logo.svg",
      "_appFooter": "<span>Generated by <strong>DocFX</strong></span><br/><span>Provided and maintained by <strong><a href='https://github.com/paulloz/godot-csharp-api/' target='_blank'>paulloz</a></strong></span>",
      "_enableSearch": true
    },
    "template": [
      "default",
      "templates/darkfx/darkfx",
      "templates/godot"
    ],
    "markdownEngineName": "markdig"
  }
}